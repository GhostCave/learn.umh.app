{{!-- Typography --}}
{{#match @custom.font_headings "!=" "System"}}
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
{{else match @custom.font_headings "!=" "System"}}
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
{{/match}}

{{!-- Headings --}}
{{#match @custom.font_headings "DM Sans"}}
  {{> font-loader id="DM+Sans" weights="300..800" font_headings="DM Sans" fallback="sans-serif"}}
{{/match}}

{{#match @custom.font_headings "Figtree"}}
  {{> font-loader id="Figtree" weights="300..800" font_headings="Figtree" fallback="sans-serif"}}
{{/match}}

{{#match @custom.font_headings "Inter"}}
